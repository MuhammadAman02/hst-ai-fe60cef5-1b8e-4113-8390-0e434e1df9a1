{
  "name": "node_project",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "npm run prisma:generate && npm run build:ts && fastify start -l info dist/app.js",
    "build": "tsc",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run prisma:generate && npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "@prisma/client": "^6.9.0",
    "@supabase/supabase-js": "^2.50.0",
    "bcrypt": "^6.0.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "envsafe": "^2.0.3",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.31",
    "@vitest/coverage-v8": "^3.2.3",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "prisma": "^6.9.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.4.0",
    "tsx": "^4.19.4",
    "typescript": "~5.8.2",
    "vitest": "^3.2.3"
  }
}
